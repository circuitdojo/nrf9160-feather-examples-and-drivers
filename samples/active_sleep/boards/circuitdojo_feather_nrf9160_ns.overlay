&gpio0 {
	sense-edge-mask = <0xffff>;
};

&uart0 {
	/delete-property/ rx-pin;
};

&i2c1 {
	lis2dh: lis2dh@18 {
		compatible = "st,lis2dh";
		label = "LIS2DH";
		reg = <0x18>;
		/delete-property/ irq-gpios;
		disconnect-sdo-sa0-pull-up;
	};
};